[credential]
	helper = store
	helper = 
	helper = /usr/bin/git-credential-manager
	credentialStore = gpg
[init]
	defaultBranch = main
[core]
	editor = lvim
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
